{
  "name": "client",
  "private": true,
  "description": "shadow ai client",
  "version": "0.0.0",
  "author": "william",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:main": "cross-env NODE_ENV=development TS_NODE_PROJECT=./tsconfig-electron.json electronmon -r ts-node/register/transpile-only ./src/main/main.ts",
    "dev:preload": "cross-env NODE_ENV=development vite build --config ./.erb/config/vite.config.preload.dev.ts --watch",
    "line": "===================================================================",
    "dev:start": "cross-env NODE_ENV=development vite --config ./.erb/config/vite.config.renderer.prod.ts",
    "build:main": "vite build --config ./.erb/config/vite.config.main.prod.ts",
    "build:renderer": "vite build --config ./.erb/config/vite.config.renderer.prod.ts",
    "build:release": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "_prepackage": "cross-env NODE_ENV=production npm run build:release",
    "package": "cross-env NODE_ENV=production npm run build:release & electron-builder build",
    "line1": "========================web 打包====================================",
    "dev:web": "cross-env NODE_ENV=development vite --config ./.erb/config/vite.config.web.ts",
    "build:web": "vite build --config ./.erb/config/vite.config.web.ts",
    "electron:generate-icons": "electron-icon-builder --input=./src/assets/logo2.png --output=build --flatten"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.48.1",
    "@loadable/component": "^5.15.3",
    "ahooks": "^3.7.7",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "electron-debug": "^3.2.0",
    "electron-log": "4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "framer-motion": "^10.12.16",
    "immer": "^10.0.2",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "pubsub-js": "^1.9.4",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2",
    "source-map-support": "^0.5.21",
    "styled-components": "6.0.0-rc.2",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@arco-plugins/vite-react": "^1.3.3",
    "@types/loadable__component": "^5.13.4",
    "@types/pubsub-js": "^1.8.3",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "chalk": "^5.2.0",
    "detect-port": "^1.5.1",
    "electron": "24.4.0",
    "electron-builder": "^23.6.0",
    "electron-icon-builder": "^2.0.1",
    "electronmon": "^2.0.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "less": "^4.1.3",
    "postcss": "^8.4.23",
    "postcss-modules": "^6.0.0",
    "postcss-preset-env": "^8.4.1",
    "tailwindcss": "^3.3.2",
    "tsconfig": "workspace:*",
    "vite": "^4.3.2"
  }
}
